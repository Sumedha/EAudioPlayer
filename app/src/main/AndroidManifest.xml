            android:label="@string/artists" />
        <activity
            android:name=".Albums"
            android:label="@string/albums" />
        <activity
            android:name=".Songs"
            android:label="@string/songs" />
        <activity
            android:name=".Genres"
            android:label="@string/genres" />
        <activity
            android:name=".EAudioPlayer"
            android:label="@string/title_activity_eaudio_player" />
        <activity
            android:name=".BuyMusicActivity"
            android:label="Buy Music" />
        <activity
            android:name=".ConfirmationActivity"
            android:label="Confirmation" />
        <activity
            android:name=".ThankYouActivity"
            android:label="Thank you!" />
