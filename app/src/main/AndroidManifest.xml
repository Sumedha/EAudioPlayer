<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.eaudioplayer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Playlists"
            android:label="@string/playlists" />
        <activity
            android:name=".Artists"
            android:label="@string/artists" />
        <activity
            android:name=".Albums"
            android:label="@string/albums" />
        <activity
            android:name=".Songs"
            android:label="@string/songs" />
        <activity
            android:name=".Genres"
            android:label="@string/genres" />
        <activity
            android:name=".EAudioPlayer"
            android:label="@string/title_activity_eaudio_player"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>