            android:name="com.example.android.eaudioplayer.androidPayPublicKey"
            android:value="BOjsfmPb3gOPnIhKOfyKH3Kxa8XX7nzynLbRkL30oA6z2gFfKPLjCeqTmyjPe41PhJ9Hh64HP/BukqUn4DGNV3Q=" />

        <!-- Enables the Android Pay API -->
